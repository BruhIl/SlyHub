	AntiTSToggleMode.Parent = AntiTSBackground

	AntiTSToggleMode.SlotToggleButton.MouseButton1Click:Connect(function()

		if not AntiTSEnabled then
			AntiTSEnabled = true

			TS:Create(AntiTSToggleMode.SlotToggleButton, TweenInfo.new(0.5), {Position = UDim2.new(0.5, 0, 0.12, 0), BackgroundColor3 = Color3.new(0, 1, 0)}):Play()
			Lighting:WaitForChild("TS").Value = false -- Updates it
			Lighting:WaitForChild("Timestop").Value = false

		else
			AntiTSEnabled = false

			TS:Create(AntiTSToggleMode.SlotToggleButton, TweenInfo.new(0.5), {Position = UDim2.new(0, 0, 0.12, 0), BackgroundColor3 = Color3.new(1, 0, 0)}):Play()
		end

		local TSValue = Lighting:WaitForChild("TS")
		local TSLighting = Lighting:WaitForChild("Timestop")

		TSValue.Changed:Connect(function()
			if AntiTSEnabled and TSValue.Value == true then
				TSValue.Value = false
				TSLighting.Enabled = false

				local TSNotice = Notice:Clone()
				TSNotice.Parent = Notice.Parent
				TSNotice.Text = "A TimeStop was activated"
				TSNotice.Visible = true
				delay(5, function()
					TSNotice:Destroy()
				end)
			end
		end)

	end)

	-- Admin Notifier

	local AdminNotifBackground = SlotFormat:Clone()
	AdminNotifBackground.Parent = CheatArea

	AdminNotifBackground.Description.Text = "Admin Notifier:"

	local AdminNotifToggleMode = SlotToggleBackground:Clone()
	AdminNotifToggleMode.Parent = AdminNotifBackground

	AdminNotifToggleMode.SlotToggleButton.MouseButton1Click:Connect(function()

		if not AdminNotifEnabled then
			AdminNotifEnabled = true

			for i, v in pairs(Players:GetChildren()) do
				local Role = Player:GetRoleInGroup(4516503)

				if Role ~= "Member" and Role ~= "Guest" then -- If they are in group and they are not lowest ranking
					local AdminNotice = Notice:Clone()
					AdminNotice.Text = Player.Name.." is in this lobby. ("..Role..")"
					AdminNotice.Parent = Notice.Parent
					AdminNotice.Visible = true
					delay(15, function()
						AdminNotice:Destroy()
					end)
				end
			end

			TS:Create(AdminNotifToggleMode.SlotToggleButton, TweenInfo.new(0.5), {Position = UDim2.new(0.5, 0, 0.12, 0), BackgroundColor3 = Color3.new(0, 1, 0)}):Play()

		else
			AdminNotifEnabled = false

			TS:Create(AdminNotifToggleMode.SlotToggleButton, TweenInfo.new(0.5), {Position = UDim2.new(0, 0, 0.12, 0), BackgroundColor3 = Color3.new(1, 0, 0)}):Play()
		end

		Players.PlayerAdded:Connect(function(Player)

			if not AdminNotifEnabled then return end

			local PlayerRole = Player:GetRoleInGroup(4516503)
			if PlayerRole ~= "Member" and PlayerRole ~= "Guest" then -- If they are in group and they are not lowest ranking
				local AdminNotice = Notice:Clone()
				AdminNotice.Text = Player.Name.." has joined. ("..PlayerRole..")"
				AdminNotice.Parent = Notice.Parent
				AdminNotice.Visible = true
				delay(15, function()
					AdminNotice:Destroy()
				end)
			end

		end)	
	end)

	-- SpeedHacks

	local SpeedHackBackground = SlotFormat:Clone()
	SpeedHackBackground.Parent = CheatArea

	local SpeedEntryBox = Entrybox:Clone()
	SpeedEntryBox.Parent = SpeedHackBackground

	SpeedHackBackground.Description.Text = "Walk Speed:"

	SpeedEntryBox:GetPropertyChangedSignal("Text"):Connect(function()

		if not tonumber(SpeedEntryBox.Text) then return end -- Makes Sure the entry is a number

		Humanoid.WalkSpeed = tonumber(SpeedEntryBox.Text)

	end)

	-- JumpHacks

	local JumpHackBackground = SlotFormat:Clone()
	JumpHackBackground.Parent = CheatArea

	local JumpEntryBox = Entrybox:Clone()
	JumpEntryBox.Parent = JumpHackBackground

	JumpHackBackground.Description.Text = "Jump Height:"

	JumpEntryBox:GetPropertyChangedSignal("Text"):Connect(function()

		if not tonumber(JumpEntryBox.Text) then return end -- Makes Sure the entry is a number

		Humanoid.JumpPower = tonumber(JumpEntryBox.Text)
		Humanoid.JumpHeight = tonumber(JumpEntryBox.Text)

	end)


repeat task.wait() until game:IsLoaded()
local Players = game:GetService("Players")

local RunService = game:GetService("RunService")
local StarterGui = game:GetService("StarterGui")
local WrongGame = false
local Duplicate = false

if RunService:IsStudio() then

	StarterGui:SetCore("SendNotification", {
		Title = "Notice:";
		Text = "You cannot run this script in studio!";
		Button1 = "Sorry";
		Duration = 600;
	})
	task.wait(0.5)
	script:Destroy()
end

local PlaceId = tostring(game.PlaceId)
if PlaceId ~= "2686500207" then
	
	StarterGui:SetCore("SendNotification", {
		Title = "Notice:";
		Text = "You tried to manually inject the incorrect script for this game. Just use the loader, it's easier for you. The link to it has been copied";
		Button1 = "Ok!";
		Duration = 600;
	})
	
	setclipboard("https://raw.githubusercontent.com/BruhIl/SlyHub/main/Loader")

	script:Destroy()
	WrongGame = true
end

local WrappedGui = game.CoreGui:FindFirstChild("MultiPurposeGUI")
if WrappedGui ~= nil then
	Duplicate = true
	
	StarterGui:SetCore("SendNotification", {
		Title = "Notice:";
		Text = "You tried to manually inject the script while another instance was active. Just use the loader. The link to it has been copied";
		Button1 = "Ok!";
		Duration = 600;
	})
	
	setclipboard("https://raw.githubusercontent.com/BruhIl/SlyHub/main/Loader")
	
	script:Destroy()
end

